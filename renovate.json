{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "npm:unpublishSafe"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        ".*"
      ],
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch"
      ],
      "enabled": true
    }
  ],
  "rangeStrategy": "bump"
}